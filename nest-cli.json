{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/rabbit-practice/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/rabbit-practice/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/rabbit-practice",
  "projects": {
    "rabbit-practice": {
      "type": "application",
      "root": "apps/rabbit-practice",
      "entryFile": "main",
      "sourceRoot": "apps/rabbit-practice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rabbit-practice/tsconfig.app.json"
      }
    },
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    },
    "payment-ms": {
      "type": "application",
      "root": "apps/payment-ms",
      "entryFile": "main",
      "sourceRoot": "apps/payment-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-ms/tsconfig.app.json"
      }
    }
  }
}